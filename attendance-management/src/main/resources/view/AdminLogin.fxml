<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="310.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep11.app.controller.AdminLoginController">
   <children>
      <Label layoutX="48.0" layoutY="40.0" prefHeight="16.0" prefWidth="389.0" text="Admin Login">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="100.0" layoutY="132.0" text="User Name: " />
      <Label layoutX="100.0" layoutY="175.0" text="Password: " />
      <TextField fx:id="txtName" layoutX="200.0" layoutY="128.0" prefHeight="24.0" prefWidth="224.0" />
      <TextField fx:id="txtPassword" layoutX="200.0" layoutY="171.0" prefHeight="24.0" prefWidth="224.0" />
      <Button fx:id="btnSignIn" layoutX="210.0" layoutY="228.0" mnemonicParsing="false" onAction="#btnSignInOnAction" prefHeight="24.0" prefWidth="81.0" text="Sign in" />
      <Button fx:id="btnSignUp" layoutX="312.0" layoutY="228.0" mnemonicParsing="false" onAction="#btnSignUpOnAction" prefHeight="24.0" prefWidth="81.0" text="Sign up" />
   </children>
</AnchorPane>
